{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Dominominal",
  "version": "0.1.0",
  "identifier": "com.lea-reift.dominominal",
  "build": {
    "frontendDist": "../public",
    "devUrl": "http://localhost:8000",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Dominominal",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "zoomHotkeysEnabled": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
        "./resources/app",
        "./resources/php"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
